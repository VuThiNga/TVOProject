# platform :ios, '9.0'

target 'TVOProject' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TVOProject
  pod 'SwiftyUserDefaults', '~> 5.0'  # Access local storage
  pod 'IQKeyboardManagerSwift', '~> 6.5.6'  # keyboard management
  pod 'Moya',             '~> 14.0'   # support call API, auto install Alamofire
  pod 'ObjectMapper', '~> 3.5'
  pod 'MBProgressHUD',    '~> 1.2.0'  # loading indicator
end
